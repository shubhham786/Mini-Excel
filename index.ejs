<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style.css">
    <title>Document</title>
</head>

<body>
    <!-- <h1>Hello from Electron</h1> -->
    <!-- page size= -->
    <div class="menu">
        <div class="file-options">File</div>
        <div class="formatting">Format</div>
        <div class="alignment">Alginment</div>
    </div>
    <div class="formula-bar">
        <input type="text" id="address">
        <input type="text">
    </div>
    <div class="main" id="main">
        <div class="top-left-box" id="top-left-box"></div>
        <div class="top-row" id="top-row">
            <%for(let i=0;i<26;i++){%>
                <div class="cell">
                    <%=String.fromCharCode(i+65)%>
                </div>
                <%}%>
        </div>
        <div class="top-left-col" id="left-col"> 
            <%for(let j=1;j<=100;j++){%>
                <div class="cell">
                    <%=j%>
                </div>
                <%}%>
        </div>
        <div class="grid">
            <% for(let i=0; i <100; i++) {%>
                <div class="row">
                    <% for(let j=0; j < 26; j++) {%>
                        <div class="cell"  rid=<%=i%>   cid=<%=j%> >

                         
                          
                        </div>
                        <% }%>

                        </div>
                                        
                <% } %>
        </div>
    </div>
    <script
  src="https://code.jquery.com/jquery-3.6.0.min.js"
  integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
  crossorigin="anonymous">
  </script>

<script src="render.js"></script>
</body>

</html>